{
    "version": 2,
    "builds": [
      {
        "src": "api/json-server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/json-server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  